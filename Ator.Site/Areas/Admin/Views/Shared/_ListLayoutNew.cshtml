<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="/lib/layui-v2.5.5/css/layui.css" media="all">
    <link rel="stylesheet" href="/css/public.css?v=@DateTime.Now.DayOfYear" media="all">
    @RenderSection("Heads", required: false)
</head>
<body>
    <div class="layuimini-container">
        <div class="layuimini-main">
            @RenderBody()
        </div>
    </div>
    <environment include="Development">
        <script src="~/lib/jquery/jquery-1.12.4.min.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="http://ajax.aspnetcdn.com/ajax/jquery/jquery-1.12.4.min.js"
                asp-fallback-src="~/lib/jquery/jquery-1.12.4.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
        </script>
    </environment>
    <script src="~/lib/layui-v2.5.5/layui.js" charset="utf-8"></script>
    <script src="~/js/lay-config.js?v=@DateTime.Now.DayOfYear" charset="utf-8"></script>
    <script src="~/js/laylist.js?v=@DateTime.Now.DayOfYear"></script>
    <script src="~/lib/xm-select/xm-select.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
